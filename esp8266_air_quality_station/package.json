{
  "scripts": {
    "watch": "webpack watch",
    "test-webpage": "webpack && npm run download-alpine && node test_server.js",
    "build-webpage": "webpack",
    "compile": "arduino-cli compile -b esp8266:esp8266:d1:xtal=80,vt=flash,exception=legacy,ssl=all,eesz=4M1M,ip=lm2f,dbg=Disabled,lvl=None____,wipe=none,baud=921600",
    "compile-demo": "arduino-cli compile -b esp8266:esp8266:d1:xtal=80,vt=flash,exception=legacy,ssl=all,eesz=4M1M,ip=lm2f,dbg=Disabled,lvl=None____,wipe=none,baud=921600 --build-property=compiler.cpp.extra_flags=-DDEMO_MODE=1",
    "upload": "arduino-cli upload -b esp8266:esp8266:d1 -p /dev/ttyUSB0",
    "release": "npm run build-webpage && npm run compile && npm run upload",
    "download-alpine": "curl -o - https://cdn.jsdelivr.net/gh/alpinejs/alpine@v2.8.2/dist/alpine.min.js | gzip > build/alpine.js.gz",
    "create-fs-data-dir": "npm run build-webpage && npm run download-alpine && cp build/alpine.js.gz data/alpine.js.gz && gzip -c build/app.css > data/app.css.gz",
    "make-fs": "npm run create-fs-data-dir && ~/.arduino15/packages/esp8266/tools/mklittlefs/2.5.0-4-fe5bb56/mklittlefs -c data -p 256 -b 8192 -s 1028096 build/fs.bin",
    "upload-fs": "npm run make-fs && ~/.arduino15/packages/esp8266/hardware/esp8266/2.7.4/tools/esptool/esptool.py -p /dev/ttyUSB0 write_flash 0x300000 build/fs.bin",
    "console": "stty 115200 -F /dev/ttyUSB0 raw -echo | cat /dev/ttyUSB0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.5",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^7.1.2",
    "core-js": "^3.10.1",
    "css-loader": "^5.0.2",
    "express": "^4.17.1",
    "express-static-gzip": "^2.1.1",
    "extract-loader": "^5.1.0",
    "html-webpack-inline-source-plugin": "1.0.0-beta.2",
    "html-webpack-plugin": "^5.2.0",
    "mini-css-extract-plugin": "^1.3.8",
    "postcss": "^8.2.9",
    "postcss-discard-comments": "^5.0.0",
    "postcss-loader": "^5.0.0",
    "purgecss": "^4.0.3",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.8",
    "sass-loader": "^8.0.2",
    "style-loader": "^2.0.0",
    "underscore": "^1.13.1",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "websocket": "^1.0.33"
  },
  "dependencies": {
    "@gfx/zopfli": "^1.0.15",
    "alpinejs": "^2.8.0",
    "bootstrap": "^5.0"
  }
}
